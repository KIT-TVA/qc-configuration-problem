<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<featureModel>
	<properties>
		<calculations key="tautology" value="true"/>
		<calculations key="auto" value="true"/>
		<calculations key="redundant" value="true"/>
		<calculations key="constraints" value="true"/>
		<calculations key="features" value="true"/>
	</properties>
	<struct>
		<and abstract="true" mandatory="true" name="DB">
			<alt abstract="true" mandatory="true" name="OS">
				<feature name="NutOS"/>
				<feature name="Win"/>
			</alt>
			<alt abstract="true" mandatory="true" name="BufferMgr">
				<and abstract="true" name="Persistent">
					<alt abstract="true" mandatory="true" name="MemAlloc">
						<feature name="Static"/>
						<feature name="Dynamic"/>
					</alt>
					<alt abstract="true" mandatory="true" name="PageRepl">
						<feature name="LRU"/>
						<feature name="LFU"/>
					</alt>
				</and>
				<feature name="InMemory"/>
			</alt>
			<feature name="DebugLogging"/>
			<and abstract="true" mandatory="true" name="Storage">
				<or abstract="true" mandatory="true" name="API">
					<feature name="get"/>
					<feature name="put"/>
					<feature name="delete"/>
				</or>
				<feature name="BTree"/>
				<feature name="Unindexed"/>
			</and>
		</and>
	</struct>
	<constraints>
		<rule>
			<conj>
				<disj>
					<not>
						<var>Storage</var>
					</not>
					<disj>
						<var>BTree</var>
						<var>Unindexed</var>
					</disj>
				</disj>
				<disj>
					<not>
						<var>BTree</var>
					</not>
					<not>
						<var>Unindexed</var>
					</not>
				</disj>
			</conj>
		</rule>
	</constraints>
</featureModel>
